   "source": [
    "#average of numbers in a tuple\n",
    "t= tuple(input('Enter the numbers seperated by comma: ').split(','))\n",
    "l=len(t)\n",
    "c=0\n",
    "for i in range(l):\n",
    "    c=int(t[i])+c\n",
    "    avg=c/l\n",
    "print(\"the average of the above given numbers are:\",avg)\n",
    "    "
